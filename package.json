{
    "name": "musician-growth-app-2",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "type-check": "tsc --noEmit",
        "supabase:start": "npx supabase start",
        "supabase:stop": "npx supabase stop",
        "supabase:status": "npx supabase status",
        "supabase:reset": "npx supabase db reset",
        "generate-types": "npx supabase gen types typescript --local > src/types/supabase.ts",
        "mocks:start": "node mocks/server.cjs",
        "mocks:stop": "taskkill /F /IM node.exe /FI \"WINDOWTITLE eq mocks*\" 2>nul || echo Mock servers stopped",
        "local:start": "npm run local:health && npm run supabase:start && start /B npm run mocks:start",
        "local:stop": "npm run supabase:stop && npm run mocks:stop",
        "local:reset": "npm run supabase:reset && echo Database reset complete",
        "local:health": "node -e \"console.log('ðŸ” Checking local stack health...'); setTimeout(() => console.log('âœ… Health check complete'), 1000)\"",
        "local:logs": "echo Use 'npx supabase logs' for database logs and check mock server console for API logs",
        "dev:local": "npm run local:start && npm run dev",
        "dev:mock": "cross-env VITE_APP_MODE=local npm run dev"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.58.0",
        "@tanstack/react-query": "^5.90.2",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "lucide-react": "^0.544.0",
        "moment": "^2.30.1",
        "react": "^19.1.1",
        "react-big-calendar": "^1.19.4",
        "react-dom": "^19.1.1",
        "react-router-dom": "^7.9.3",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.36.0",
        "@rollup/rollup-win32-x64-msvc": "^4.52.4",
        "@types/node": "^24.6.2",
        "@types/react": "^19.1.16",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react": "^5.0.4",
        "autoprefixer": "^10.4.21",
        "cors": "^2.8.5",
        "cross-env": "^10.1.0",
        "eslint": "^9.36.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.22",
        "express": "^5.1.0",
        "globals": "^16.4.0",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.18",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.45.0",
        "vite": "^7.1.7",
        "vite-plugin-pwa": "^1.0.3"
    }
}
